<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/bg">

    <include
        android:id="@+id/relativeLayout1"
        layout="@layout/activity_header">

    </include>

    <ImageButton
        android:id="@+id/imgbtn_logout_dashboard_xml"
        android:layout_width="@dimen/_25sdp"
        android:layout_height="@dimen/_25sdp"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/relativeLayout1"
        android:layout_marginBottom="0dp"
        android:layout_marginEnd="20dp"
        android:contentDescription="@null"
        android:src="@drawable/logout" />

    <TextView
        android:id="@+id/tv_login_username_dashboard_xml"
        style="@style/navi_text_style"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:textSize="@dimen/_8sdp"
        android:layout_alignTop="@+id/imgbtn_logout_dashboard_xml"
        android:layout_marginStart="10dp"
        android:layout_marginTop="@dimen/_8sdp"
        android:text="@string/welcome" />

    <LinearLayout
        android:id="@+id/ll_dashbrd1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imgbtn_logout_dashboard_xml"
        android:layout_centerHorizontal="true"
        android:layout_margin="5dp"
        android:weightSum="3"
        android:baselineAligned="false">

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            android:gravity="center"
            android:layout_margin="5dp">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                tools:ignore="UselessParent"
                android:gravity="center">

            <android.support.v7.widget.AppCompatImageView
                android:id="@+id/img_DrunkDrive"
                android:layout_width="@dimen/_40sdp"
                android:layout_height="@dimen/_40sdp"
                android:src="@drawable/drunkdrive"
                android:elevation="@dimen/_20sdp"
                android:layout_centerHorizontal="true"/>

            <android.support.v7.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="@dimen/_40sdp"
                app:cardElevation="0dp"
                app:cardCornerRadius="@dimen/_5sdp"
                android:layout_marginTop="@dimen/_30sdp">

                <TextView
                    android:id="@+id/tv_generateticket_dashboard_xml"
                    style="@style/navi_text_style"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal|center_vertical"
                    android:text="@string/generate_ticket"
                    android:visibility="visible"
                    android:textSize="@dimen/_10sdp"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/_5sdp"/>

            </android.support.v7.widget.CardView>
            </RelativeLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            android:gravity="center"
            android:layout_margin="5dp">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                tools:ignore="UselessParent"
                android:gravity="center">

                <android.support.v7.widget.AppCompatImageView
                    android:id="@+id/img_Spot"
                    android:layout_width="@dimen/_40sdp"
                    android:layout_height="@dimen/_40sdp"
                    android:src="@drawable/spot"
                    android:elevation="@dimen/_20sdp"
                    android:layout_centerHorizontal="true"/>

                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_40sdp"
                    app:cardElevation="0dp"
                    app:cardCornerRadius="@dimen/_5sdp"
                    android:layout_marginTop="@dimen/_30sdp">

                    <TextView
                        android:id="@+id/tv_spot_dashboard_xml"
                        style="@style/navi_text_style"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal|center_vertical"
                        android:text="@string/spot_challan"
                        android:visibility="visible"
                        android:textSize="@dimen/_10sdp"
                        android:layout_gravity="center"
                        android:layout_marginTop="@dimen/_5sdp"/>

                </android.support.v7.widget.CardView>
            </RelativeLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            android:gravity="center"
            android:layout_margin="5dp">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                tools:ignore="UselessParent"
                android:gravity="center">

                <android.support.v7.widget.AppCompatImageView
                    android:id="@+id/img_VehicleHistory"
                    android:layout_width="@dimen/_40sdp"
                    android:layout_height="@dimen/_40sdp"
                    android:src="@drawable/vehiclehistory"
                    android:elevation="@dimen/_20sdp"
                    android:layout_centerHorizontal="true"/>

                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_40sdp"
                    app:cardElevation="0dp"
                    app:cardCornerRadius="@dimen/_5sdp"
                    android:layout_marginTop="@dimen/_30sdp">

                    <TextView
                        android:id="@+id/tv_vhclehistory_dashboard_xml"
                        style="@style/navi_text_style"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal|center_vertical"
                        android:text="@string/vehicle_history"
                        android:visibility="visible"
                        android:textSize="@dimen/_10sdp"
                        android:layout_gravity="center"
                        android:layout_marginTop="@dimen/_5sdp"/>

                </android.support.v7.widget.CardView>
            </RelativeLayout>
        </LinearLayout>

    </LinearLayout>


    <LinearLayout
        android:id="@+id/linearLayout1"
        style="@style/margin_top_btm_style"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/ll_dashbrd1"
        android:layout_centerHorizontal="true"
        android:layout_margin="5dp"
        android:weightSum="3">


        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            android:layout_margin="5dp">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                tools:ignore="UselessParent"
                android:gravity="center">

                <android.support.v7.widget.AppCompatImageView
                    android:id="@+id/img_CraneAct"
                    android:layout_width="@dimen/_40sdp"
                    android:layout_height="@dimen/_40sdp"
                    android:src="@drawable/crane"
                    android:elevation="@dimen/_20sdp"
                    android:layout_centerHorizontal="true"/>

                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_40sdp"
                    app:cardElevation="0dp"
                    app:cardCornerRadius="@dimen/_5sdp"
                    android:layout_marginTop="@dimen/_30sdp">

                    <TextView
                        android:id="@+id/tv_towing_dashboard_xml"
                        style="@style/navi_text_style"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal|center_vertical"
                        android:text="@string/towing"
                        android:visibility="visible"
                        android:textSize="@dimen/_10sdp"
                        android:layout_gravity="center"
                        android:layout_marginTop="@dimen/_5sdp"/>

                </android.support.v7.widget.CardView>
            </RelativeLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            android:layout_margin="5dp">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                tools:ignore="UselessParent"
                android:gravity="center">

                <android.support.v7.widget.AppCompatImageView
                    android:id="@+id/img_ReleaseDoc"
                    android:layout_width="@dimen/_40sdp"
                    android:layout_height="@dimen/_40sdp"
                    android:src="@drawable/release"
                    android:elevation="@dimen/_20sdp"
                    android:layout_centerHorizontal="true"/>

                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_40sdp"
                    app:cardElevation="0dp"
                    app:cardCornerRadius="@dimen/_5sdp"
                    android:layout_marginTop="@dimen/_30sdp">

                    <TextView
                        android:id="@+id/tv_releasedocuments_dashboard_xml"
                        style="@style/navi_text_style"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal|center_vertical"
                        android:text="@string/release_documents"
                        android:visibility="visible"
                        android:textSize="@dimen/_10sdp"
                        android:layout_gravity="center"
                        android:layout_marginTop="@dimen/_5sdp"/>

                </android.support.v7.widget.CardView>
            </RelativeLayout>

        </LinearLayout>


        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            android:layout_margin="5dp">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                tools:ignore="UselessParent"
                android:gravity="center">

                <android.support.v7.widget.AppCompatImageView
                    android:id="@+id/img_Reports"
                    android:layout_width="@dimen/_40sdp"
                    android:layout_height="@dimen/_40sdp"
                    android:src="@drawable/reports"
                    android:elevation="@dimen/_20sdp"
                    android:layout_centerHorizontal="true"/>

                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_40sdp"
                    app:cardElevation="0dp"
                    app:cardCornerRadius="@dimen/_5sdp"
                    android:layout_marginTop="@dimen/_30sdp">

                    <TextView
                        android:id="@+id/tv_reports_dashboard_xml"
                        style="@style/navi_text_style"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal|center_vertical"
                        android:text="@string/reports"
                        android:visibility="visible"
                        android:textSize="@dimen/_10sdp"
                        android:layout_gravity="center"
                        android:layout_marginTop="@dimen/_5sdp"/>

                </android.support.v7.widget.CardView>
            </RelativeLayout>

        </LinearLayout>


    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout3"
        style="@style/margin_top_btm_style"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/linearLayout1"
        android:layout_centerHorizontal="true"
        android:layout_margin="5dp"
        android:weightSum="3">


        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            android:layout_margin="5dp">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                tools:ignore="UselessParent"
                android:gravity="center">

                <android.support.v7.widget.AppCompatImageView
                    android:id="@+id/img_DuplicatePrint"
                    android:layout_width="@dimen/_40sdp"
                    android:layout_height="@dimen/_40sdp"
                    android:src="@drawable/duplicateprint"
                    android:elevation="@dimen/_20sdp"
                    android:layout_centerHorizontal="true"/>

                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_40sdp"
                    app:cardElevation="0dp"
                    app:cardCornerRadius="@dimen/_5sdp"
                    android:layout_marginTop="@dimen/_30sdp">

                    <TextView
                        android:id="@+id/tvduplicateprint_dashboad_xml"
                        style="@style/navi_text_style"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal|center_vertical"
                        android:text="@string/duplicate_print"
                        android:visibility="visible"
                        android:textSize="@dimen/_10sdp"
                        android:layout_gravity="center"
                        android:layout_marginTop="@dimen/_5sdp"/>

                </android.support.v7.widget.CardView>
            </RelativeLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            android:layout_margin="5dp">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                tools:ignore="UselessParent"
                android:gravity="center">

                <android.support.v7.widget.AppCompatImageView
                    android:id="@+id/img_DownldMastr"
                    android:layout_width="@dimen/_40sdp"
                    android:layout_height="@dimen/_40sdp"
                    android:src="@drawable/sync"
                    android:elevation="@dimen/_20sdp"
                    android:layout_centerHorizontal="true"/>

                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_40sdp"
                    app:cardElevation="0dp"
                    app:cardCornerRadius="@dimen/_5sdp"
                    android:layout_marginTop="@dimen/_30sdp">

                    <TextView
                        android:id="@+id/tv_sync_dashboard_xml"
                        style="@style/navi_text_style"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal|center_vertical"
                        android:text="@string/sync"
                        android:visibility="visible"
                        android:textSize="@dimen/_10sdp"
                        android:layout_gravity="center"
                        android:layout_marginTop="@dimen/_5sdp"/>

                </android.support.v7.widget.CardView>
            </RelativeLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            android:layout_margin="5dp">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                tools:ignore="UselessParent"
                android:gravity="center">

                <android.support.v7.widget.AppCompatImageView
                    android:id="@+id/img_Settings"
                    android:layout_width="@dimen/_40sdp"
                    android:layout_height="@dimen/_40sdp"
                    android:src="@drawable/settings"
                    android:elevation="@dimen/_20sdp"
                    android:layout_centerHorizontal="true"/>

                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_40sdp"
                    app:cardElevation="0dp"
                    app:cardCornerRadius="@dimen/_5sdp"
                    android:layout_marginTop="@dimen/_30sdp">

                    <TextView
                        android:id="@+id/tv_settings_dashboard_xml"
                        style="@style/navi_text_style"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal|center_vertical"
                        android:text="@string/settings"
                        android:visibility="visible"
                        android:textSize="@dimen/_10sdp"
                        android:layout_gravity="center"
                        android:layout_marginTop="@dimen/_5sdp"/>

                </android.support.v7.widget.CardView>
            </RelativeLayout>

        </LinearLayout>


    </LinearLayout>


    <LinearLayout
        android:id="@+id/linearLayout4"
        style="@style/margin_top_btm_style"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/linearLayout3"
        android:layout_centerHorizontal="true"
        android:layout_margin="5dp"
        android:weightSum="3">

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            android:layout_margin="5dp">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                tools:ignore="UselessParent"
                android:gravity="center">

                <android.support.v7.widget.AppCompatImageView
                    android:id="@+id/img_About"
                    android:layout_width="@dimen/_40sdp"
                    android:layout_height="@dimen/_40sdp"
                    android:src="@drawable/about"
                    android:elevation="@dimen/_20sdp"
                    android:layout_centerHorizontal="true"/>

                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_40sdp"
                    app:cardElevation="0dp"
                    app:cardCornerRadius="@dimen/_5sdp"
                    android:layout_marginTop="@dimen/_30sdp">

                    <TextView
                        android:id="@+id/tv_about_version"
                        style="@style/navi_text_style"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal|center_vertical"
                        android:text="@string/about_version"
                        android:visibility="visible"
                        android:textSize="@dimen/_10sdp"
                        android:layout_gravity="center"
                        android:layout_marginTop="@dimen/_5sdp"/>

                </android.support.v7.widget.CardView>
            </RelativeLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            android:visibility="invisible"
            android:layout_gravity="center"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                tools:ignore="UselessParent"
                android:gravity="center">

                <android.support.v7.widget.AppCompatImageView
                    android:id="@+id/img_Specil"
                    android:layout_width="@dimen/_40sdp"
                    android:layout_height="@dimen/_40sdp"
                    android:src="@drawable/spot"
                    android:elevation="@dimen/_20sdp"
                    android:layout_centerHorizontal="true"/>

                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_30sdp"
                    app:cardElevation="10dp"
                    app:cardMaxElevation="10dp"
                    app:cardCornerRadius="@dimen/_5sdp"
                    android:layout_marginTop="@dimen/_30sdp">

                    <TextView
                        android:id="@+id/tv_special_drive"
                        style="@style/navi_text_style"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal|center_vertical"
                        android:text="@string/special_drive"
                        android:visibility="visible"
                        android:textSize="@dimen/_10sdp"
                        android:layout_gravity="center"/>

                </android.support.v7.widget.CardView>
            </RelativeLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            android:gravity="center"
            android:visibility="invisible"
            >

            <android.support.v7.widget.AppCompatImageView
                android:layout_width="@dimen/_55sdp"
                android:layout_height="@dimen/_55sdp"
                android:src="@drawable/noncontact"
                />

            <TextView
                android:id="@+id/tv_echallannon"

                style="@style/navi_text_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal|center_vertical"
                android:text="@string/echallanfornoncontact"
                android:visibility="invisible"
                android:textSize="@dimen/_10sdp" />
        </LinearLayout>




    </LinearLayout>

    <TextView
        android:id="@+id/textView3"
        style="@style/footer_style"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:gravity="center"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:textStyle="bold"/>



</RelativeLayout>