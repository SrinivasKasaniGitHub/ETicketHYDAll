    <ImageButton
        android:id="@+id/imgbtn_logout_dashboard_xml"
        android:layout_width="@dimen/_25sdp"
        android:layout_height="@dimen/_25sdp"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/relativeLayout1"
        android:layout_marginBottom="0dp"
        android:layout_marginEnd="20dp"
        android:contentDescription="@null"
        android:src="@drawable/logout" />

    <TextView
        android:id="@+id/tv_login_username_dashboard_xml"
        style="@style/navi_text_style"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:textSize="@dimen/_8sdp"
        android:layout_alignTop="@+id/imgbtn_logout_dashboard_xml"
        android:layout_marginStart="10dp"
        android:layout_marginTop="@dimen/_8sdp"
        android:text="@string/welcome" />

    <LinearLayout
        android:id="@+id/ll_dashbrd1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imgbtn_logout_dashboard_xml"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="0dp"
        android:weightSum="3">

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            android:gravity="center">

            <android.support.v7.widget.AppCompatImageView
                android:id="@+id/img_DrunkDrive"
                android:layout_width="@dimen/_55sdp"
                android:layout_height="@dimen/_55sdp"
                android:src="@drawable/generate_ticket"/>

            <TextView
                android:id="@+id/tv_generateticket_dashboard_xml"
                style="@style/navi_text_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal|center_vertical"
                android:text="@string/generate_ticket"
                />
        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            android:gravity="center"
            >

            <android.support.v7.widget.AppCompatImageView
                android:id="@+id/img_Spot"
                android:layout_width="@dimen/_55sdp"
                android:layout_height="@dimen/_55sdp"
                android:src="@drawable/spot_challan"
               />

            <TextView
                android:id="@+id/tv_spot_dashboard_xml"
                style="@style/navi_text_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal|center_vertical"
                android:text="@string/spot_challan"

                />
        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            android:gravity="center"
            >

            <android.support.v7.widget.AppCompatImageView
                android:id="@+id/img_VehicleHistory"
                android:layout_width="@dimen/_55sdp"
                android:layout_height="@dimen/_55sdp"
                android:src="@drawable/vhcle_history"
                />

            <TextView
                android:id="@+id/tv_vhclehistory_dashboard_xml"
                style="@style/navi_text_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:text="@string/vehicle_history" />
        </LinearLayout>









    </LinearLayout>

    <TextView
        android:id="@+id/textView3"
        style="@style/footer_style"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:gravity="center"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:textStyle="bold"/>

    <LinearLayout
        android:id="@+id/linearLayout1"
        style="@style/margin_top_btm_style"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/ll_dashbrd1"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="5dp"
        android:weightSum="3">


        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            android:gravity="center"
            >

            <android.support.v7.widget.AppCompatImageView
                android:id="@+id/img_CraneAct"
                android:layout_width="@dimen/_55sdp"
                android:layout_height="@dimen/_55sdp"
                android:src="@drawable/towing"
                />

            <TextView
                android:id="@+id/tv_towing_dashboard_xml"
                style="@style/navi_text_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal|center_vertical"
                android:text="@string/towing" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            android:gravity="center"
            >

            <android.support.v7.widget.AppCompatImageView
                android:id="@+id/img_ReleaseDoc"
                android:layout_width="@dimen/_55sdp"
                android:layout_height="@dimen/_55sdp"
                android:src="@drawable/releasedocument"
                />

            <TextView
                android:id="@+id/tv_releasedocuments_dashboard_xml"
                style="@style/navi_text_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal|center_vertical"
                android:text="@string/release_documents" />
        </LinearLayout>


        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            android:gravity="center"
            >

            <android.support.v7.widget.AppCompatImageView
                android:id="@+id/img_Reports"
                android:layout_width="@dimen/_55sdp"
                android:layout_height="@dimen/_55sdp"
                android:src="@drawable/reports"
                />

            <TextView
                android:id="@+id/tv_reports_dashboard_xml"
                style="@style/navi_text_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal|center_vertical"
                android:text="@string/reports" />
        </LinearLayout>


    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout3"
        style="@style/margin_top_btm_style"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/linearLayout1"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="0dp"
        android:weightSum="3">


        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            android:gravity="center"
            >

            <android.support.v7.widget.AppCompatImageView
                android:id="@+id/img_DuplicatePrint"
                android:layout_width="@dimen/_55sdp"
                android:layout_height="@dimen/_55sdp"
                android:src="@drawable/print"
                />

            <TextView
                android:id="@+id/tvduplicateprint_dashboad_xml"
                style="@style/navi_text_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal|center_vertical"
                android:text="@string/duplicate_print" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            android:gravity="center"
            >

            <android.support.v7.widget.AppCompatImageView
                android:id="@+id/img_DownldMastr"
                android:layout_width="@dimen/_55sdp"
                android:layout_height="@dimen/_55sdp"
                android:src="@drawable/sync"
                />

            <TextView
                android:id="@+id/tv_sync_dashboard_xml"
                style="@style/navi_text_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal|center_vertical"
                android:text="@string/sync"
                />
        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            android:gravity="center"
            >

            <android.support.v7.widget.AppCompatImageView
                android:id="@+id/img_Settings"
                android:layout_width="@dimen/_55sdp"
                android:layout_height="@dimen/_55sdp"
                android:src="@drawable/settings"
                />

            <TextView
                android:id="@+id/tv_settings_dashboard_xml"
                style="@style/navi_text_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal|center_vertical"
                android:text="@string/settings" />
        </LinearLayout>


    </LinearLayout>

